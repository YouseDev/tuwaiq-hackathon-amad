# ElevenLabs TTS Configuration
# Copy this file to .env and fill in your actual values

# Get your API key from: https://elevenlabs.io/app/speech-synthesis/text-to-speech
EXPO_PUBLIC_ELEVENLABS_API_KEY=sk-your-actual-api-key-here

# Get voice ID from ElevenLabs voice library (e.g., "21m00Tcm4TlvDq8ikWAM")
EXPO_PUBLIC_ELEVENLABS_VOICE_ID=your-chosen-voice-id

# API Configuration (usually dont need to change)
EXPO_PUBLIC_ELEVENLABS_API_URL=https://api.elevenlabs.io/v1/text-to-speech
EXPO_PUBLIC_ELEVENLABS_MODEL_ID=eleven_multilingual_v2

# Voice Quality Settings (0.0 - 1.0)
EXPO_PUBLIC_TTS_STABILITY=0.5
EXPO_PUBLIC_TTS_SIMILARITY_BOOST=0.5
EXPO_PUBLIC_TTS_STYLE=0.0
EXPO_PUBLIC_TTS_SPEAKER_BOOST=true
